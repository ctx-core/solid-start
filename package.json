	"dependencies": {
		"ctx-core": "^7.1.4",
		"solid-js": "^1.8.16"
	},
	"devDependencies": {
		"@ctx-core/http": "9.5.171",
		"@solidjs/meta": "^0.29.3",
		"@types/aws-lambda": "^8.10.137",
		"aws-lambda": "^1.0.7",
		"c8": "^9.1.0",
		"solid-start": "^0.3.11",
		"tsx": "^4.7.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
